<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.pearl.mapper.RewardMapper">

	<insert id="insert">
		insert into
		REWARD(RWRDNUM,RWRDNAME,FUNDNUM,RWRDPRICE)
		values(REWARD_SEQ.nextval,#{rwrdName},#{FUND_SEQ.nextval},#{rwrdprice});
	</insert>

</mapper>